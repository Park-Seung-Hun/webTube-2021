extends ../layouts/main
include ../mixins/videoBlock

block content
    .search__header 
        h3 검색어 : #{searchingBy}
    .search__videos 
        if videos.length === 0 
            h5 비디오가 존재하지 않습니다.
        each video in videos   
            +videoBlock({
                title: video.title,
                views:video.views,
                videoFile:video.fileUrl,
                id:video.id
            })    

